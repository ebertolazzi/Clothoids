<Group view="code" labelreference="L19" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># =============================================
# DEFINIZIONE DELLA CHEBYQUAD FUNCTION
# =============================================

# Polinomi di Chebyshev shiftati T_i^*(x) = T_i(2x-1)
ChebyshevShifted := proc(i, x)
    return orthopoly[T](i, 2*x-1);
end proc:

# Integrale di T_i^*(t) da 0 a 1
ChebyshevIntegral := proc(i)
    local t;
    if i = 0 then
        return 1;
    elif i mod 2 = 1 then
        return 0;
    else
        return 1/(1 - i^2);
    end if;
end proc:

# Sistema Chebyquad per dimensione n
ChebyquadSystem := proc(n)
    local system_proc;
    
    system_proc := proc(x)
    local f, i, j, sum_val;
        f := Vector(n);
        for i from 1 to n do
            sum_val := 0;
            for j from 1 to n do
                sum_val := sum_val + ChebyshevShifted(i, x[j]);
            end do;
            f[i] := (1/n) * sum_val - ChebyshevIntegral(i);
        end do;
        return f;
    end proc;
    
    return system_proc;
end proc:</Text-field>
</Input>
</Group>
