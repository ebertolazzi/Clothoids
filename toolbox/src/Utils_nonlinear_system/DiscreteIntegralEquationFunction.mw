<Group view="code" labelreference="L7" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Definizione della funzione del sistema discreto
DiscreteSystem := proc(n, x)
local h, k, tk, j, tj, sum1, sum2, f;
    h := 1.0 / (n + 1);
    f := Vector(n);
    for k from 1 to n do
        tk := k * h;
        sum1 := 0;
        # Prima somma: j da 1 a k-1
        for j from 1 to k-1 do
            tj := j * h;
            sum1 := sum1 + tj * (x[j] + tj + 1)^3;
        end do;
        sum2 := 0;
        # Seconda somma: j da k a n
        for j from k to n do
            tj := j * h;
            sum2 := sum2 + (1 - tj) * (x[j] + tj + 1)^3;
        end do;
        f[k] := x[k] + h * ((1 - tk) * sum1 + tk * sum2) / 2;
    end do;
    return convert(f,list);
end proc:</Text-field>
</Input>
</Group>
